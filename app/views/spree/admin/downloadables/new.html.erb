<%= form_for [:admin, @product, @downloadable], :html => { :multipart => true } do |f| %>
  <fieldset>
  <legend>New downloadable file</legend>

  <table class="basic-table" data-hook="add_downloadable">
    <%= render :partial => 'form', :locals => { :f => f } %>
  </table>

  <p class="form-buttons">
    <%= button t("update") %>
    or <%= link_to t("cancel"), "#", :id => "cancel_link" %>
  </p>
  </fieldset>
<% end %>